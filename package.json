  "scripts": {
    "start": "run-p start:*",
    "start:json-server": "json-server --watch ./src/data/db.json --port 5002",
    "start:react-scripts": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --passWithNoTests",
    "eject": "react-scripts eject",
